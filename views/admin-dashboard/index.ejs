                            <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/195612/chat_avatar_01_green.jpg"
                                alt="avatar" />
                        </div>
                        <!-- end chat-header -->
                        <div class="chat-box clearfix">
                            <textarea style="width: 470px; height: 450px;" name="message-to-send" id="message-to-send"
                                rows="3"></textarea>
                        </div>
                        <div class="chat-message clearfix">
                            <textarea style="width: 400px;" name="message-to-send" id="message-to-send"
                                placeholder="Type your message" rows="3"></textarea>
                            <button>Send</button>
                        </div>
                    </div>
                </div>
                <!-- end chat-message -->
            </div>
            <!-- end chat -->
        </div>
